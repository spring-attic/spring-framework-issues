<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
                                     http://java.sun.com/xml/ns/persistence/orm_1_0.xsd" version="1.0">
    <!-- orm.xml for SCRATCH PROJECT -->
   
    <!-- In-memory database testing will fail if we set a schema - JPA does not generate a CREATE SCHEMA command. -->
	<!-- JPA will prefix table names in annotated queries, with the schema name set here. -->
	<persistence-unit-metadata>
		<persistence-unit-defaults>
			<schema></schema>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
    
</entity-mappings>